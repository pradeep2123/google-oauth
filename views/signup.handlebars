    </head>
    <body class="text-center" style="margin-top:80px">
        <div class="card">
            <center>
            <div class="row">
                <div class= "col-lg-8">
                    <form class="form-signin" method="POST" style="width:300px" action="/user/create">
                    <div class="container">
                        <center>
                            {{#if  user}}
                            <div class="alert alert-success">
                                <strong>{{user}}</strong>
                            </div>
                            {{/if}}
                        </center>
                        <h1 class="h3 mb-3 font-weight-normal">Signup</h1>
                        {{#if  error}}
                        <div class="alert alert-danger">
                            <strong>{{error}}</strong>
                        </div>
                        {{/if}}
                        <div class="form-group" align="left">
                        <label><strong> First Name</strong></label><input type="text" id="first-name" class="form-control" name="first_name"placeholder=" First Name" required="" autofocus="">
                        </div>
                        <div class="form-group" align="left">
                        <label for="last-name"><strong>Last Name</strong></label>
                        <input type="text" id="last-name" class="form-control" name="last_name"placeholder="Last Name" required="" autofocus="">
                        </div>
                        <div class="form-group" align="left">
                        <label for="Email"><strong>Email</strong></label>
                        <input type="email" id="Email" class="form-control" name="email"placeholder="Email" required="" autofocus="">
                        </div>
                        <div class="form-group" align="left">
                        <label for="password"><strong>Password</strong></label>
                        <input type="password" id="password" pattern="^([a-zA-Z0-9@*#]{8,15})$" title="password should between 8 and 15 characters" class="form-control" name="password"placeholder="Password" required="" autofocus="">
                        </div>
                        <div class="form-group" align="left">
                        <label for="mobilenumber"><strong>Mobile Number</strong></label>
                        <input type="number" id="mobilenumber" pattern="^(\+\d{1,3}[- ]?)?\d{10}$" title="mobile number should be 10 digits" class="form-control" name="mobile_number"placeholder="Mobile Number" required="" autofocus="">
                    </div>
                    </form>
                    <div class="row">
                        <div class="col-md-4">
                            <button class="btn btn-primary" style="margin-right:15px"  type="submit">Signup</button>
                        </div>
                            <div class="col-md-4">
                                <a href="/auth/google/" class="btn btn-danger"><span class="fa fa-google-plus"></span>Google</a>
                            </div>
                        <div class="col-md-2">
                            <a id="signin"href="/user/signin" class="btn btn-alert">Signin</a>
                        </div>
                    </div>
                </div>
                
            </div>
            </center>
        </div>
      </div>
      <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
